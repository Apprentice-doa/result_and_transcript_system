CREATE TABLE `result_and_transcript_system`.`results` ( `result_id` INT NOT NULL , `s_n` INT NOT NULL , `c_a` INT NOT NULL , `exam` INT NOT NULL , `total` INT NOT NULL , `rules` TEXT NOT NULL , `course_idr` INT NOT NULL , `session_idr` INT NOT NULL ) ENGINE = InnoDB;
CREATE TABLE `result_and_transcript_system`.`courses` ( `course_id` INT NOT NULL , `course_code` TEXT NOT NULL , `course_unit` INT NOT NULL , `course_title` TEXT NOT NULL , `course_semesrer` INT NOT NULL ) ENGINE = InnoDB;
CREATE TABLE `result_and_transcript_system`.`department` ( `dept_name` TEXT NOT NULL , `dept_faculty` TEXT NOT NULL , `comments` TEXT NOT NULL ) ENGINE = InnoDB;
CREATE TABLE `result_and_trancript_db`.`dynamic_elm` ( `hod` TEXT NOT NULL , `lecturer` TEXT NOT NULL , `course_id` INT NOT NULL , `course_title` TEXT NOT NULL ) ENGINE = InnoDB;
CREATE TABLE `result_and_transcript_system`.`session_idr` ( `session_idr` INT NOT NULL , `session_year` INT NOT NULL , `comments` INT NOT NULL ) ENGINE = InnoDB;
CREATE TABLE `result_and_trancript_system`.`students` ( `student_id` INT NOT NULL , `student_matno` TEXT NOT NULL , `student_firstname` TEXT NOT NULL , `student_surname` TEXT NOT NULL , `student_othernames` INT NOT NULL , `student_title` TEXT NOT NULL , `student_deptidr` INT NOT NULL ) ENGINE = InnoDB;
